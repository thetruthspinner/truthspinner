termux-setup-storage
clear
install='\033[1;32m'
echo "${install}Installing..."
pkg install zip -y
pkg install unzip -y
cp ngrok "/data/data/com.termux/files/usr/bin/"
cd "/data/data/com.termux/files/usr/bin/"
